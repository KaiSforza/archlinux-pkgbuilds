# Contributor: FallenWizard <fallenwizard@archlinux.us>
pkgname=ompload-git
pkgver=20121020
pkgrel=1
pkgdesc="Script to upload files to omploader.org"
arch=("any")
url="http://omploader.org"
license=('GPL')
depends=('ruby' 'curl')
makedepends=('git')
provides=('ompload')
conflicts=('ompload')
source=("git://git.omp.am/omploader.git")
md5sums=('SKIP') 

build() {
  cd $srcdir/omploader
  install -D -m755 ompload ${pkgdir}/usr/bin/ompload
} 
